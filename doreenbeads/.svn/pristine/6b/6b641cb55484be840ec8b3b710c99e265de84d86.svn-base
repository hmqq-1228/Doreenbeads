	function showAuto() {
		n = n >=(count - 1) ? 0 : ++n;
		$j("#idSlider li").filter(":visible").fadeOut(time1).parent().children().eq(n).fadeIn(time1); 
		$j("#idnumsolider li").eq(n).addClass("on").siblings().removeClass("on");
	} 
