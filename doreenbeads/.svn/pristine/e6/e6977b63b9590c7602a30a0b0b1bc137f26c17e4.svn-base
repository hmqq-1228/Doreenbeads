.find-back,.addresscancel-btn{width:40%;float:right;font-size:14px;height:30px;}
.backbtn{float:left;font-size:14px;line-height:26px;padding:0 20px;margin-left:5px;}
.advanced_two{cursor:pointer;display:inline-block;font-size:14px;border-radius:3px;line-height:24px;margin-top:5px;width:100%;}
