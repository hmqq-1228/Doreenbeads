.button-now{line-height:26px;}
.button-now,.find-submit,.checkbtn-cart,.goshop-btn{cursor:pointer;display:inline-block;width:100%;font-size:14px;color:#fff;text-align:center;font-weight:bold;}
.checkbtn-cart{float:right;width:44%;line-height:26px;}
.goshop-btn{display:block;width:70%;margin-top:8px;}


.advanced_one{display:inline-block;font-size:14px;border-radius:3px;line-height:24px;margin-top:5px;width:100%;color:#fff;text-align:center;font-weight:bold;}

