.newto{padding:0 28px 15px 28px;}
.newto p{text-align:center;line-height:20px;margin:10px 0;}

.button-change,.find-back,.backbtn,.addresscancel-btn,.advanced_two{cursor:pointer;display:block;background:#ededed;background:-webkit-gradient(linear,0 0,0 100%,from(#f5f5f5),to(#e6e6e6));background:-moz-linear-gradient(top,#f5f5f5,#e6e6e6);background:-o-linear-gradient(top,#f5f5f5,#e6e6e6);border:1px solid #bdbdbd;box-shadow:0px 1px 0px #a8a8a8;color:#333;font-weight:bold;text-align:center;text-decoration:none;}
.button-change{width:100%;font-size:16px;line-height:30px;}
