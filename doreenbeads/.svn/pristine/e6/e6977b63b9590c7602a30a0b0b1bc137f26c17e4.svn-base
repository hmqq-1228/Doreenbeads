.current-2,.current-3{background:url(../images/shopcartbg.png) center -280px no-repeat;background-size:375px 458px;}
.current-2.now,.current-3.now{background:url(../images/shopcartbg.png) center -206px no-repeat;background-size:375px 457px;}
.current-4{background:url(../images/shopcartbg.png) center -330px no-repeat;background-size:375px 457px;}
.current-4.now{background:url(../images/shopcartbg.png) center -167px no-repeat;background-size:375px 458px;}




