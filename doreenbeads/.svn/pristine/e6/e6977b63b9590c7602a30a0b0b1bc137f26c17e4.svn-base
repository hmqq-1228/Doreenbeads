.useitbtn,.button-now,.find-submit,.checkbtn-cart,.goshop-btn,.viewallbtn,.viewlessbtn,.deletesubmit-btn,.advanced_one{background:#b760d4;border:1px solid #974baf;padding:2px 0;background:-moz-linear-gradient(top,#c971e6,#b260cd);background:-webkit-gradient(linear,0 0,0 100%,from(#c971e6),to(#b260cd));background:-o-linear-gradient(top,#c971e6,#b260cd);}
.addresssubmit-btn{background:#ec4384;border:1px solid #c33269;}
