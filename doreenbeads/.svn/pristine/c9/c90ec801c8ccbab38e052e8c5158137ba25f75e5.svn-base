@charset "utf-8";
/* CSS Document */
input,select,button,textarea{font-family:Arial, Helvetica, sans-serif;}
body,h1,h2,h3,h4,h5,h6,p,ul,ol,figure,textarea,input,button,select,form,table,td,tr,dl,dd,dt,option{margin:0;padding:0;}
body{-webkit-tap-highlight-color:rgba(255,255,255,0);-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;-webkit-touch-callout:none;cursor:default;-ms-content-zooming:none;-webkit-text-size-adjust:100%}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:400}
address,caption,cite,code,dfn,em,th,del,ins,mark,sup,sub,var{font-style:normal;font-weight:400;text-decoration:none;background:0;font-size:100%;vertical-align:middle}
article,aside,details,figcaption,figure,footer,header,hgroup,main,nav,section,summary{display:block}
ul,ol{padding-left:0;list-style-type:none}
input,select{vertical-align:middle;user-select:auto}
input{word-break:normal}
a{outline:none;text-decoration:none;-webkit-tap-highlight-color:rgba(0,0,0,0);}
fieldset,img{border:0}
/* table{border-collapse:collapse;border-spacing:0} */
body{background:#FFF;font-family:Arial, Helvetica, sans-serif;/*font-size:12px;*/}
h1,h2,h3,div,li,p,menu,button{font-size:100%;-webkit-tap-highlight-color:transparent}
a{color:#3b5998;-webkit-tap-highlight-color:rgba(0,0,0,0)}
.clearfix{display:block;content:'';clear:both;visibility:hidden}
input,select,textarea{outline:none;}
textarea{resize:none;}
a:hover{text-decoration:underline;}
.head-notice{height: 32px;background: #fff7ec;line-height: 32px;margin-top: 5px;color: #f98549;padding-left: 12px;width: 640px;margin:0 auto;}
.head-notice marquee{width: 80%;}
.head-notice marquee span{display: inline-block;color: #f98549;}
.head-notice ins{display: inline-block;}
.head-notice ins.notice{width: 17px;height: 41px;background:url(../images/iconbg.png) -166px -573px no-repeat;margin-right: 5px;margin-top: 9px;}
.head-notice ins.close{width: 10px;height: 41px;background:url(../images/iconbg.png) -129px -575px no-repeat;margin:9px 5px 0 10px;}

.header-top{background:#e9e7e7;border-bottom:1px solid #cccccc;padding:3px 0;height:auto;overflow:hidden;}
.header-top select{border:1px solid #ccc;height:18px;margin:0 10px;float:left;height:auto;overflow:hidden;font-size:12px;width:90px;}
.head-portrait{display:inline-block;width:30px;height:30px;background:url(../images/iconbg.png) -125px -2px;margin-left:40px;}
.shopcart{display:inline-block;width:30px;height:30px;background:url(../images/iconbg.png) -2px -4px;}
.shopcart1{display:inline-block;width:30px;height:30px;background:url(../images/iconbg.png) -282px -140px;}
.like-collect{display:inline-block;width:34px;height:34px;background:url(../images/iconbg.png) -84px -4px;}
.header-logo{padding:0 5px;margin-top:5px;}
.header-logo table{cellpadding:0;cellspacing:0;border-collapse:collapse;width:100%;}
.header-logo table td{vertical-align:bottom;}
.header-logo table td .logo{display:inline-block;width:150px;height:30px;background:url(../images/logo_old.jpg) no-repeat;background-size:150px;}
.menushow,.menuclose{display:inline-block;border:1px solid #ccc;height:16px;line-height:16px;background:#fff;padding:0 10px;float:right;margin-right:5px;cursor:pointer;}
.menushow ins{display:inline-block;height:0;width:0;border-top:5px solid #000;border-bottom:5px solid transparent;border-left:5px solid transparent;border-right:5px solid transparent;position:relative;top:2px;left:2px;}
.mainmenu{position:absolute;width:100%;top:0;z-index:300;background:#f2f3f2;padding-bottom:10px;}
.menuclose ins{display:inline-block;height:0;width:0;border-top:5px solid transparent;border-bottom:5px solid #000;border-left:5px solid transparent;border-right:5px solid transparent;position:relative;top:-3px;left:2px;}

.newsletter{padding-top:20px;}
.searchform,.subscribeform{padding:0 5px;margin-top:5px;height:auto;overflow:hidden;height:36px;}
.searchform div{position:relative;margin-right:78px;}
.subscribenotice{padding:5px 5px;color:#ff0000;}
.subscribeform div{position:relative;margin-right:68px;}
.searchform input[type="text"],.subscribeform input[type="text"]{background:#fff;font-size:14px;position:absolute;height:34px;line-height:34px;width:100%;border:1px solid #914ba8;padding-left:10px;font-family:Arial, Helvetica, sans-serif;color:#9a9a9a;}
.searchform input[type="Submit"],.subscribeform input[type="Submit"]{float:right;height:36px;width:66px;border:1px solid #914ba8;background:#a264b5;border-left:0;color:#fff;font-weight:bold;font-family:Arial, Helvetica, sans-serif;font-size:13px;cursor:pointer;}

.searchform input[type="Submit"]{float:right;height:36px;width:67px;border:1px solid #914ba8;background:#a264b5 url(../images/search.png) 20px 4px no-repeat;background-size:26px 26px;border-left:0;color:#fff;font-weight:bold;font-family:Arial, Helvetica, sans-serif;font-size:14px;}

.subscribeform input[type="Submit"]{width:80px;}
.subscribeform input[type="text"]{padding-left:10px;background-size:25px 21px;}
.subscribeform div{margin-right:92px;}
.searchform input.writefocus,.subscribeform input.writefocus{color:#333;font-style:normal;}

nav{padding:0 5px;line-height:20px;margin:8px 0;color:#a766ad;font-size:14px;}
nav a{padding:0 2px 0 3px;color:#333333;}
nav strong{color:#0066ff;}
.listcontent{padding-bottom:10px;}
.listcontent-tit{background:#f7f7f7;height:30px;line-height:30px;border-top:1px solid #949494;padding:0 10px;}
.listcontent-tit li{float:left;margin-right:7px;}
.listcontent-tit li form{padding:5px;}
.listcontent-tit li select{font-size:12px;border:1px solid #CCCCCC;}
.refine-by-btn{padding:3px 5px;background:-moz-linear-gradient(top,#c971e6,#b260cd);background:-webkit-gradient(linear,0 0,0 100%,from(#c971e6),to(#b260cd));background:-o-linear-gradient(top,#c971e6,#b260cd);border:1px solid #974baf;border-radius:2px;box-shadow:1px 1px 1px #c9c9c9;color:#fff;}
.refine-by-btn ins{display:inline-block;border-left:4px solid #fff;border-top:4px solid transparent;border-bottom:4px solid transparent;border-right:4px solid transparent;height:0;width:0;overflow:hidden;margin-left:5px;}
.listcontent-tit .refinebtn {float:right;}

.orange{color:#ee5823;}
.listcontent-head{position:relative;height:28px;line-height:28px;margin:10px 20px 0px 20px;}
.listcontent-head h1{position:absolute;left:0;height:29px;}
.listcontent-head h1.list-change{background:url(../images/iconbg.png) -1px -108px no-repeat;}
.listcontent-head h1.gallery-change{background:url(../images/iconbg.png) -1px -139px no-repeat;}
.listcontent-head h1 a.list-link{display:inline-block;width:33px;height:28px;}
.listcontent-head h1 a.gallery-link{display:inline-block;width:33px;height:28px;}
.listcontent-head div{right:0;text-align:right;}
.prev-page{border:1px solid #646464;padding:7px 16px;color:#000;margin-right:10px;background:url(../images/iconbg.png) -351px -10px;background-size:200px 200px;}
.next-page{border:1px solid #646464;padding:7px 16px;color:#000;margin-left:10px;background:url(../images/iconbg.png) -371px -10px;background-size:200px 200px;}
.listcontent-detail {position:relative;}
.listcontent-detail .line{border-bottom:1px solid #ccc;position:relative;height:12px;}
.listcontent-detail .line span.top{position:absolute;display:inline-block;border-bottom:6px solid #CCC;border-top:6px solid transparent;border-left:6px solid transparent;border-right:6px solid transparent;top:0;left:30px;}
.listcontent-detail .line span.bot{position:absolute;display:inline-block;border-bottom:5px solid #fff;border-top:5px solid transparent;border-left:6px solid transparent;border-right:5px solid transparent;top:3px;left:30px;}
.listcontent-detail .line.gallery span.top{left:65px;}
.listcontent-detail .line.gallery span.bot{left:65px;}

.category-content{display:block;position:relative;padding:10px 4px 10px 165px;height:auto;overflow:hidden;border-bottom:1px dotted #cccccc;min-height:170px;margin:0 10px;}
.category-content .category-content-img{display:inline-block;border:1px solid #cccccc;position:absolute;left:0;width:143px;height:143px;text-align:center;}
.category-content .category-content-img ins{display:inline-block;text-decoration:underline;color:#003399;}
.category-content ul li{margin-bottom:10px;overflow: hidden;word-wrap: break-word;line-height:16px;}
.category-content ul li a{color:#555;}
.category-content ul li.oldprice{color:#888;padding-top:2px;text-decoration:line-through;margin-bottom:0;}
.category-content ul li.nowprice{font-weight:bold;color:#c12e2e;}
.category-content ul li input{border:1px solid #d0cfcf;width:60px;height:22px;}
.category-content ul li a.addcart-btn{display:inline-block;width:52%;background:-moz-linear-gradient(top,#f4db5b,#f5c647);background:-webkit-gradient(linear,0 0,0 100%,from(#f4db5b),to(#f5c647));background:-o-linear-gradient(top,#f4db5b,#f5c647);border:1px solid #d7a23d;padding:0px 5px;line-height:22px;color:#623500;font-weight:bold;margin-right:10px;text-align:center;box-shadow:0 1px 0 0 #f7e586 inset, 0 0 0 1px #f7e282 inset,1px 1px 1px #d0cfcf;}
.category-content ul li a.update-btn{display:inline-block;width:52%;background:-moz-linear-gradient(top,#f4db5b,#f5c647);background:-webkit-gradient(linear,0 0,0 100%,from(#f4db5b),to(#f5c647));background:-o-linear-gradient(top,#f4db5b,#f5c647);border:1px solid #d7a23d;padding:0px 5px;line-height:22px;color:#623500;font-weight:bold;margin-right:10px;text-align:center;box-shadow:0 1px 0 0 #f7e586 inset, 0 0 0 1px #f7e282 inset,1px 1px 1px #d0cfcf;}
.category-content ul li a.update-btn ins{display:inline-block;width:20px;height:20px;background:url(../images/iconbg.png) no-repeat -235px -42px;}
.category-content ul li a.addcollect-btn,.category-content ul li a.addcollect-btn-success{display:inline-block;width:20%;background:#f6f6f6;border:1px solid #d3d3d3;padding:2px 5px;box-shadow:1px 1px 1px #d0cfcf;text-align:center;}
.category-content ul li a.addcollect-btn span{display:inline-block;width:14px;height:13px;background:url(../images/iconbg.png) no-repeat -182px -44px;position:relative;top:3px;}
.category-content ul li a.addcollect-btn-success span{display:inline-block;width:14px;height:13px;background:url(../images/iconbg.png) no-repeat -211px -44px;position:relative;top:3px;}

.addsuccess-content{position:absolute;z-index:1001;width:100%;display:none;}
.addsuccess-tip{background:rgba(0,0,0,0.6);color:#fff;margin-top:-150px;text-align:center;padding:20px 5%;font-size:14px;line-height:26px}
.addsuccess-tip a{color:#e9d12e;text-decoration:underline;font-weight:bold;}
.addwish-content{position:absolute;margin:0 0.5%;z-index:1001;width:99%;display:none;}
.addwish-tip{background:rgba(0,0,0,0.6);color:#fff;margin-top:-150px;text-align:center;padding:20px 5%;font-size:14px;line-height:26px}
.addwish-tip a{color:#e9d12e;text-decoration:underline;font-weight:bold;}
.restock-content{position:absolute;margin:0 0.5%;z-index:1001;width:99%;display:none;}
.restock-tip{background:rgba(0,0,0,0.6);color:#fff;margin-top:-150px;text-align:center;padding:20px 5%;font-size:14px;line-height:26px}
.restock-tip a{color:#e9d12e;text-decoration:underline;font-weight:bold;}
.error-content{position:absolute;margin:0 0.5%;z-index:1001;width:99%;display:none;}
.error-tip{background:rgba(0,0,0,0.6);color:#fff;margin-top:-150px;text-align:center;padding:20px 5%;font-size:14px;line-height:26px}


.back-to-top a{display:block;width:100%;font-size:14px;text-align:center;font-weight:bold;color:#2b2b2b;height:20px;line-height:20px;margin:10px 0;text-decoration:underline;}
.back-to-top a ins{display:inline-block;width:0;height:0;overflow:hidden;border-bottom:6px solid #2b2b2b;border-top:6px solid transparent;border-left:6px solid transparent;border-right:6px solid transparent;position:relative;top:-2px;left:2px;}

.footer-menu{border-top:1px solid #a3a3a3;}
.footer-menu li{background:#e9e9e9;height:38px;line-height:38px;border-bottom:1px solid #a3a3a3;border-top:1px solid #fff;}
.footer-menu li a{display:block;font-size:16px;color:#555;position:relative;}
.footer-menu li a ins{margin:0 18px 0 40px;display:inline-block;width:30px;height:26px; }
.footer-menu li.home ins{background:url(../images/iconbg.png) 0 -40px no-repeat;}
.footer-menu li.cart ins{background:url(../images/iconbg.png) 0 -8px no-repeat;}
.footer-menu li.account ins{background:url(../images/iconbg.png) -123px -10px no-repeat;}
.footer-menu li.contact ins{background:url(../images/iconbg.png) -160px -10px no-repeat;}
.footer-menu li:hover{background:#f2f3f2;}
.footer-menu li span{color:#224ea7;}

footer{margin-top:10px;background:#f4f6f3;}
.detail-associated h3{background:#f7f7f7;border-top:1px solid #569218;line-height:28px;color:#030303;padding:0 5px;font-size:14px;font-weight:bold;}
.login-register{text-align:center;margin-top:10px;}
.login-register a{color:#0b71ca;font-weight:bold;}
.facebookcont{background:#fff;}
/*.facebook{background:url(../images/facebook.png) 0px 0;background-size:320px 40px;width:202px;height:40px;margin:0 auto;padding-left:118px;line-height:40px;font-size:13px;color:#3f5699;font-weight:bold;}*/

footer nav{color:#555;text-align:center;padding:0 30px;margin-top:10px;}
footer nav a{color:#555;padding:0 3px;}
footer section{width:100%;text-align:center;}
.footer-bot-txt{text-align:center;color:#686868;border-top:1px solid #666;padding-top:5px;padding-bottom:10px;}


/*index.html*/
.side-nav{margin:0px 1px 10px;border:1px solid #ccc;}
.side-nav h3{background:#f5f5f5;color:#fff;height:42px;line-height:42px;font-weight:bold;font-size:16px;padding:0 30px;}
.side-nav ul li a{display:block;padding:0 20px;color:#010101;line-height:42px;font-size:14px;border-bottom:1px solid #d1d1d1;background:#f5f5f5;border-top:1px solid #e6e6e6;}
.side-nav ul li a:hover{background:#fff;}
.side-nav ul li a span{display:inline-block;width:15px;float:right;background:url(../images/iconbg.png) -55px -0px;background-size:200px 200px;}


/*gallery.html page*/
.dlgallery{padding:10px 0px 10px 0px;height:auto;overflow:hidden;}
.dlgallery ul{padding:0 3px;display: block;height: auto;position: relative;margin:0 auto;overflow:hidden;}
.dlgallery ul li input{border:1px solid #d3d5d1;height:26px;width:100px;color:#000;padding:0 5px;margin-left:4px;display:inline-block;float:left;}
.dlgallery-img{display:inline-block;width:143px;height:143px;border:1px solid #ccc;}
.dlgallery-text{display:block;width:136px;color:#033333;padding:0 6px;margin:0 auto;text-align:left;font-size:12px;}
.dlgallery ul li{float:left;width:50%;margin-bottom:10px;height:auto;overflow:hidden;text-align:center;border-bottom:1px dotted #ccc;height:315px;}
.dlgallery ul li .oldprice{display:block;width:136px;color:#888;text-decoration:line-through;padding:4px 6px 0;line-height:20px;font-size:12px;text-align:left;margin:0 auto;line-height:20px;}
.dlgallery ul li .nowprice{display:block;width:136px;margin:0 auto;color:#d20c0c;padding:0 6px;font-size:12px;text-align:left;line-height:20px;}
.addcart{display:inline-block;width:98%;background:#b760d4;border:1px solid #974baf;padding:4px 0;background:-moz-linear-gradient(top,#c971e6,#b260cd);background:-webkit-gradient(linear,0 0,0 100%,from(#c971e6),to(#b260cd));background:-o-linear-gradient(top,#c971e6,#b260cd);margin-top:5px;color:#fff;font-size:14px;font-weight:bold;line-height:22px;text-align:center;}
.addcollect{display:inline-block;line-height:30px;float:left;color:#555;font-size:12px;padding-left:6px;}
.cartcont{width:145px;height:auto;clear:both;overflow:hidden;padding-top:4px;margin:0 auto;position:relative;}
.soldoutbtn{display:inline-block;background:#ebebeb;color:#333;width:98%;margin:5px 6px 0 0;text-align:center;padding:4px 0;line-height:20px;border:1px solid #bdbdbd;font-size:14px;font-weight:bold;background:-moz-linear-gradient(top,#f5f5f5,#e6e6e6);background:-webkit-gradient(linear,0 0,0 100%,from(#f5f5f5),to(#e6e6e6));background:-o-linear-gradient(top,#f5f5f5,#e6e6e6);box-shadow:1px 1px 0 #ccc;}

.soldtext{font-size:12px;height:28px;line-height:10px;}
.dlgallery ul li .cartcont-restock{width:145px;margin:0 auto;}
.updatecart{display:inline-block;width:98%;background:#ec4383;padding:4px 0;border:1px solid #c33269;margin-top:5px;color:#fff;font-size:14px;font-weight:bold;line-height:22px;text-align:center;}
#addsuccess-tip{margin-top:0;}

.listcontent-head div.pagecenter{width:100%;text-align:center;}

/*detail.html page*/
.product-descript{line-height:18px;color:#000;margin-bottom:5px;}
.product-price strong{color:#c12e2e;}
.detailcontent .detail_info{padding:10px 5px;border-top:1px solid #ccc;}
.detailcontent{margin-bottom:25px;}
.pricetab{cellpadding:0;cellspacing:0;width:100%;margin-top:20px;}
.pricetab td{width:16%;text-align:center;border:1px solid #cccccc;padding:7px 1px;font-size:14px;color:#333;}
.detailproduct-btn {margin:10px 0 0 10px;}
.detailproduct-btn button{width:45%;cursor:pointer;}

.detailproduct-btn .updatecart{background:#ec4383;display:inline-block;border:1px solid #C33269;color:#fff;font-weight:bold;font-size:14px;margin:0 4% 0 0;text-align:center;}
.detailproduct-btn .addwishilist-btn{display:inline-block;color:#333;background:-webkit-gradient(linear,0 0,0 100%,from(#f6f6f6),to(#efefef));background:-moz-linear-gradient(top,#f6f6f6,#efefef);background:-o-linear-gradient(top,#f6f6f6,#efefef);border:1px solid #d3d3d3;box-shadow:0 1px 0 0 #f7e586 inset, 0 0 0 1px #f7e282 inset;color:#3333;text-align:center;box-shadow:1px 1px 0px #d0cfcf;font-size:14px;border-radius:2px;line-height:22px;padding:4px 0;margin-right:4%;font-weight:bold;}
.detailproduct-btn .detail-restock{margin-top:5px;}
.detailproduct-btn .detail-restock a{font-size:14px;}
.detailproduct-btn span{display:block;color:#663300;font-weight:bold;font-size:14px;}
.detailproduct-btn .preorder-btn strong,.detailproduct-btn .update-btn strong{background-size:200px 300px;}
.detail-descript{padding:10px 5px 10px 5px;margin:20px 0 10px 0;height:auto;overflow:hidden;border-top:1px solid #ccc;}
.detail-descript p{line-height:20px;color:#333;}
.detail-descript table{cellpadding:0;cellspacing:0; width:100%;margin-top:10px;}
.detail-descript table td{border:1px solid #ccc;text-align:center;padding:5px 10px;color:#555;}
.detail-descript-list {margin-top:20px;height:auto;overflow:hidden;}
.detail-descript-list li{border:1px solid #ccc;margin-top:5px;line-height:24px;}
.detail-descript-list li a{display:block;color:#555;padding:0 5px;position:relative;}
.detail-descript-list li a:hover{background:#ccc;text-decoration:none;}
.detail-descript-list li a em{display:inline-block;position:absolute;right:5px;top:6px;width:8px;height:12px;background:url(../images/iconbg.png) -55px -13px;background-size:200px 200px;}
.detail-descript-list li ins{display:inline-block;width:22px;height:16px;background:url(../images/iconbg.png) -64px -77px;margin-left:10px;background-size:200px 200px;}
.detail-descript-list li span.goldstar{background:url(../images/iconbg.png) -35px -75px;}
.detail-descript-list li span.greystar{background:url(../images/iconbg.png) -4px -75px;}
.detail-descript-list li span.greystar,.detail-descript-list li span.goldstar{display:inline-block;width:18px;height:20px;position:relative;top:3px;background-size:200px 200px;}
.detailproduct-btn .addcart{margin-right:4%;}

.titleh3{background:#f4f4f4;border-top:1px solid #cccccc;line-height:28px;padding:0 5px;font-size:14px;font-weight:bold;margin-bottom:20px;}
.viewmore{text-align:center;margin-top:10px;font-size:16px;font-weight:bold;color:#0066ff;}
.detail-associated  p.viewmore a{color:#003399;font-weight:bold;font-size:12px;text-decoration:underline;}
.detail-input{margin:10px 10px 0 10px;}
.detail-input button{display:inline-block;border:1px solid #d5d5d5;width:22px;height:22px;text-align:center;font-weight:bold;font-size:14px;color:#545454;margin-left:10px;position:relative;top:1px;box-shadow:1px 1px 1px #dedede;background:#eee;background:-moz-linear-gradient(top,#f5f5f5,#e6e6e6);background:-webkit-gradient(linear,0 0,0 100%,from(#f5f5f5),to(#e6e6e6));cursor:pointer;}
.detail-input input{display:inline-block;border:1px solid #d0d0d0;height:22px;width:30%;margin-left:10px;box-shadow:1px 1px 1px #dedede;text-align:center;}


/*refine by*/
/*refine by*/
.windowbg{background:#000;left: 0;position: absolute;top: 0; width: 100%;z-index: 200;opacity:0.4;display:none;}
.refine-right{position:absolute;top:0;right:0;width:82%;z-index:201;border:1px solid #ccc;display:none;}
.refine-right .refine-tit{line-height:40px;height:40px;border-bottom:1px solid #cccccc;border-top:1px solid #fff;padding-left:15px;text-align:left;font-size:14px;color:#010101;background:#f2f3f2;cursor:pointer;}
.refine-right .refine-tit > span{display:inline-block;width:10px;height:10px;background:url(../images/iconbg.png) no-repeat -144px -250px;background-size:200px 300px;float:right;position:relative;top:12px;right:10px;}
.refine-right h1{font-size:16px;background:#dadada;line-height:36px;padding-left:15px;}
.refine-right ul{background:#fff;display:none;}
.refine-right ul li > a{display:block;padding-left:20px;line-height:34px;color:#010101;font-size:14px;}
.refine-right ul li a > ins{font-size:20px;font-weight:bold;color:#f15824;margin-right:5px;}
.refine-right ul li a:hover {background:#ccc;}
.refine-right ul li p{text-align:right;cursor:pointer;color:#003399;font-size:14px;cursor:pointer;}
