@charset "utf-8";
html, body {height: 100%;}
body{ font-family: Verdana, Geneva, sans-serif; min-width:320px; color:#333; font-size:14px; line-height:18px; min-height:100%; display:flex; flex-flow: column nowrap; overflow-x: hidden;}
.wrap-page{ width:100%; height:100%; flex-grow:1; overflow:auto; max-width:640px; margin:0 auto;-webkit-overflow-scrolling: touch;}
.wrap-sidebar{ width:100%; height:100%; flex-grow:1; overflow:auto;  max-width:270px;-webkit-overflow-scrolling: touch;}
.showNav {
	transform: translate(270px,0);
    -webkit-transform: translate(270px,0);
	-ms-transform: translate(270px,0); /* Internet Explorer */
    -moz-transform: translate(270px,0); /* Firefox */
    -o-transform: translate(270px,0); /* Opera */
    transition: transform 500ms;
    -webkit-transition: -webkit-transform 500ms;
}
.closeNav {
	transform: translate(0,0);
    -webkit-transform: translate(0,0);
	-ms-transform: translate(0,0); /* Internet Explorer */
    -moz-transform: translate(0,0); /* Firefox */
    -o-transform: translate(0,0); /* Opera */
    transition: transform 500ms;
    -webkit-transition: -webkit-transform 500ms;
}
body,ul,li,dl,dd,dt,h1,h2,h3,h4,h5,h6,form,p,button,input,table,tr,td,h3{ padding:0; margin:0;}
h1,h2,h3,h4,h5{ font-size:1em; font-weight:normal; line-height:18px;}
ins{ text-decoration:none;}
em,i{ font-style:normal;}
a{ color:#333; text-decoration:none;}
ul,ol{ list-style-type:none;}
input{font-family: Verdana, Geneva, sans-serif; font-size:14px; outline:none;}
img {border:0;}
a { text-decoration:none;}
.float_lt{ float:right;}
.wrap{ width:100%;padding-bottom:60px; max-width:640px; }
.clearfix { clear:both;}
div,h3,table,a,li,ul,input,img{box-sizing:border-box;-moz-box-sizing:border-box; /* Firefox */-webkit-box-sizing:border-box; /* Safari */}
