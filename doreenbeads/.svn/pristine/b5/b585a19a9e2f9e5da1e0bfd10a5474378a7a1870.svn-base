.index_login_window .join_btn{ text-align:center; padding:0 10px;margin-top: 10px;}
.index_login_window .join_btn .button_join{background-color:#bc66d8;color:#fff; line-height:30px;height:30px;font-size:16px; display:inline-block; padding:0 20px;border:1px solid #974ADD;border-radius:5px;text-shadow:1px 1px 0 #8f4da5;font-weight:bold;}
.link_color{ color:#3b5998;}
	
/*站内信*/
.reminder_wrap{
    font-family:arial;    
    width: 100%;
    flex-grow: 1;
    overflow: auto;
    max-width: 640px;
    margin: 50px auto 10px auto;
    }
.coupon_reminder{margin:0 auto;}
.coupon_reminder span{display: inline-block;margin: 10px auto 10px auto;}
.coupon_title span {
    display: inline-block;
    width: 100%;
    text-align: center;
    font-size: 15px;
    color: white;
    line-height: 20px;
}
.coupon_title{
    background-color: #8E3DA8;
    width: 33%;
    height: 60px;
    display: inline-block;
    BORDER-RIGHT: 2px;
    border-right-color: #DDD2D2;
    border-right-style: inset;
}
.coupon_message_content {
    display: inline-block;
    background-color: #8E3DA8;
    margin: 0 auto;
    width: 66%;
    height: 60px;
    text-align: center;
    font-size: 10px;
    float: right;
    color: white;
    position:relative;
    line-height: 20px;
    overflow: hidden;
}
.coupon_message_content span a{
    color:white;
    text-decoration:underline;
}
.reminder_wrap .close{position:absolute;top:5px;right:5px;display:inline-block; background:url(../images/r_close.png) no-repeat 0 0;width:15px;height:15px;background-size: contain;}
