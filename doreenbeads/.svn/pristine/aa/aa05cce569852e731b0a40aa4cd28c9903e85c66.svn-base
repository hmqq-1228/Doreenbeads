#subcribe-email{padding-left:10px;}

.newto{padding:0 28px 15px 28px;}
.newto p{text-align:center;line-height:20px;margin:10px 0;}

.useitbtn,.button-now,.find-submit,.checkbtn-cart,.goshop-btn,.viewallbtn,.viewlessbtn,.deletesubmit-btn,.advanced_one{background:#b760d4;border:1px solid #974baf;padding:2px 0;background:-moz-linear-gradient(top,#c971e6,#b260cd);background:-webkit-gradient(linear,0 0,0 100%,from(#c971e6),to(#b260cd));background:-o-linear-gradient(top,#c971e6,#b260cd);}
.addresssubmit-btn{background:#ec4384;border:1px solid #c33269;}
.useitbtn{cursor:pointer;display:inline-block;font-size:14px;padding:0 30px;line-height:28px;margin-top:5px;color:#fff;font-weight:bold;}
.button-now{line-height:26px;}
.button-now,.find-submit,.checkbtn-cart,.goshop-btn{cursor:pointer;display:inline-block;width:100%;font-size:14px;color:#fff;text-align:center;font-weight:bold;}
.checkbtn-cart{float:right;width:44%;line-height:26px;}
.goshop-btn{display:block;width:70%;margin-top:8px;}

.button-change,.find-back,.backbtn,.addresscancel-btn,.advanced_two{cursor:pointer;display:block;background:#ededed;background:-webkit-gradient(linear,0 0,0 100%,from(#f5f5f5),to(#e6e6e6));background:-moz-linear-gradient(top,#f5f5f5,#e6e6e6);background:-o-linear-gradient(top,#f5f5f5,#e6e6e6);border:1px solid #bdbdbd;box-shadow:0px 1px 0px #a8a8a8;color:#333;font-weight:bold;text-align:center;text-decoration:none;}
.button-change{width:100%;font-size:16px;line-height:30px;}
.loading{width:40%;height:100px;background:#525252 url(../images/loadbig.gif) center center no-repeat;background-size:80px 80px;margin:0 auto;position:absolute;top:55px;left:28%;}
