.log-offtext{padding:0 20px 20px 20px;}
.log-offtext p{line-height:16px;margin-top:10px;}
.find-submit,.addresssubmit-btn,.deletesubmit-btn{width:40%;float:left;height:30px;font-size:14px;cursor:pointer;color:#fff;font-weight:bold;}
.find-back,.addresscancel-btn{width:40%;float:right;font-size:14px;height:30px;}
.backbtn{float:left;font-size:14px;line-height:26px;padding:0 20px;margin-left:5px;}
.advanced_two{cursor:pointer;display:inline-block;font-size:14px;border-radius:3px;line-height:24px;margin-top:5px;width:100%;}
.enternewbtn{height:28px;line-height:28px;width:50%;font-size:14px;margin-bottom:10px;}
