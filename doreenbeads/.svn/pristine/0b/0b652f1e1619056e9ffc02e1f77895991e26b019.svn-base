filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f6f6f6', endColorstr='#eeeeee', GradientType='0'); /* IE*/border-radius:3px; display:inline-block; height:28px;border:#b8b8b8 1px solid; font-size:14px; color:#000; font-family:Arial, Helvetica, sans-serif;margin-left:10px;cursor:pointer;padding:0 24px;cursor:pointer;}

.orderprocess{height:45px;margin:0px auto 20px auto;}
.orderprocess ul{height:45px;padding:0 200px;width:740px;margin:0 auto;} 
.orderprocess ul li{float:left;margin:0 auto;}
.orderprocess ul li.pend{background:url(../images/bg_step.png) center -126px;padding:16px 50px 0 50px;*padding-top:18px;}
.orderprocess ul li.process{background:url(../images/bg_step.png) center -168px;padding:21px 50px 0 50px;}
.orderprocess ul li.ship{background:url(../images/bg_step.png) center -252px;padding:21px 50px 0 50px;}
.orderprocess ul li strong{font-weight:normal;color:#999;}
.orderprocess ul li strong.greenbold{font-weight:bold;color:#3676e0;margin-right:5px;}

.orderprocess ul li.pendnext{background:url(../images/bg_step.png) center 0;padding:21px 50px 0 50px;}
.orderprocess ul li.processnext{background:url(../images/bg_step.png) center -210px;padding:16px 50px 0 50px;*padding-top:18px;}
.orderprocess ul li.shipnext{background:url(../images/bg_step.png) center -210px;padding:16px 50px 0 50px;*padding-top:18px;}
.orderprocess ul li.processthre{background:url(../images/bg_step.png) center -42px;padding:21px 50px 0 50px;}
.orderprocess ul li.shipthre{background:url(../images/bg_step.png) center -84px;padding:16px 50px 0 50px;*padding-top:18px;}
.orderprocess ul li.canceled{background:url(../images/bg_step.png) center -295px no-repeat;padding:16px 50px 0 50px;float:none;text-align:center;*padding-top:18px;}

.nowdiv{position:relative;display:inline-block;}
.orderprocess_tips{width:390px; border:1px solid #E9D3B3;position:absolute;background:#FFFDEA;top:10px;z-index:100;padding:5px;text-align:left;left:-220px;*left:-128px;display:none;}
.orderprocess_tips strong{color:#C00;font-weight:bold;padding-left:10px;}
.orderprocess_tips span{width:0; height:0; font-size:0; overflow:hidden; position:absolute;}
.orderprocess_tips span.bot{border-width:8px;border-style:dashed dashed solid dashed;border-color: transparent transparent #E9D3B3 transparent;left:200px;top:-16px;}
.orderprocess_tips span.top{border-width:8px;border-style: dashed dashed solid dashed;border-color: transparent transparent #FFFDEA transparent;left:200px;top:-15px;}

