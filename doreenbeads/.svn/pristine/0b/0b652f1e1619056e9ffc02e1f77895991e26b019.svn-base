/**********
********翻页样式
******************************************/
.propagelist{height:auto; text-align:right; position:relative; right:0px; top:0; font-size:12px; font-weight:normal;margin:5px 0;}
.propagelist a{background:#fff; overflow:auto; zoom:1; line-height:16px; color:#666;padding:3px 6px;border:1px solid #cccbcb;margin:0 3px;cursor:default;}
.propagelist a:hover{border:1px solid #aa6bb0; cursor:pointer;}
.propagelist a.current{border:none;font-weight:bold;background:none;}
.propagelist a.able{background:#e1e1e1;border:1px solid #bfbdbd;color:#e1e1e1;}
.propagelist a ins.right{display:inline-block; border-left:4px solid #6e6e6e;border-top:4px solid transparent;border-bottom:4px solid transparent;overflow:hidden;line-height:0;height:0;width:0; margin-left:5px;}
.propagelist a ins.left{display:inline-block; border-right:4px solid #6e6e6e;border-top:4px solid transparent;border-bottom:4px solid transparent;overflow:hidden;line-height:0;height:0;width:0;}

