.caption_shopgray{ height:30px; background:#e6e6e6;background:-moz-linear-gradient(top,#eeeeee,#dedede);background:-webkit-linear-gradient(top,#eeeeee,#dedede);filter: progid:DXImageTransform.Microsoft.Gradient(gradientType=0,startColorStr=#eeeeee,endColorStr=#dedede);color:#333; padding:0 10px;border:1px solid #ccc;line-height:30px;}
