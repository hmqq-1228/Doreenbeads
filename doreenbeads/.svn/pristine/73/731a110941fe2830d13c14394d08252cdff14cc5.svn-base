.caption_shopgray .level li.schedule{ width:147px; height:22px; overflow:hidden; margin:6px 10px; background:url(../images/bg_schedule.png) top no-repeat; padding-left:6px; color:#555; position:relative;}
.caption_shopgray .level li.schedule ins{ position:absolute; text-align:center; display:block; line-height:22px; height:22px; width:147px; text-align:center; text-decoration:none;color:#fff;}
.caption_shopgray .level li.schedule span{ display:block;height:22px; background:url(../images/bg_schedule.png) right -23px no-repeat; }
.caption_shopgray .level li.schedule span{ display:block;height:22px; background:url(../images/bg_schedule.png) right -23px no-repeat; }
.caption_shopgray .level li.schedule_null{ width:150px; height:22px; overflow:hidden; margin:6px 10px; background:url(../images/bg_schedule.png) 0 -47px no-repeat; padding-left:6px; color:#666; position:relative;}
.caption_shopgray .level li.schedule_null ins{ position:absolute; text-align:center; display:block; line-height:22px; height:22px; width:120px; text-align:center; text-decoration:none;}
.caption_shopgray .level li.schedule_null span{ display:block;height:22px; }
