#MenuContainer {
	background-color:#FBFAF8;
	border-radius:10px;
	-moz-border-radius:10px 10px 10px 10px;
	border:1px solid #d4d0d0;
	width:15%;
	float:left;
}
#MenuContainer h4{
	color:#000;
	font-size:15px;
	border:0px solid #f5f6f1;
	background-repeat:no-repeat;
 	cursor:auto;
	background-position:100%;
	padding:0px 5px 0px 5px;
	cursor:pointer;
	margin-left:13px;
	margin-top:8px;
}
.fen_ge{	
	height:1px;
	margin-left:20px;
	width:120px;
	margin-bottom:5px;
}
#MenuContainer hr{
	border:#bcb8af; border-bottom:1px dashed #bcb8af; clear:both;
	font-weight:bold;
}
#MenuContainer p{
	margin-left:3px;
	color:#000;
	padding-left:8px;
	font-size:13px;
	margin-top:2px;
	background-repeat:no-repeat;
	line-height:24px;
}
#MenuContainer p:hover{
	cursor:pointer;
}
#MenuContainer p.index{
	cursor:pointer;
	color:#009c43;
	font-weight:bold;
}
.left_arrow {
    color: #EB781E;
}
#myaccountBody #navBreadCrumb a{color:#000}
#myaccountBody #navBreadCrumb span{color:#008FED}
.accountfirst {
    height: auto;
    margin: 5px auto 0;
    width: 766px;
	font-family:verdana;
}
.quickadd_btn {
    text-align: right;
    line-height: 20px;
}

.quickadd_btn a {
    font-weight: bold;
	color: #008FED;
    text-decoration: none;
}
.btn_yellow {
    background: none;
    border: medium none;
    cursor: pointer;
    font-family: Tahoma;
    overflow: visible;
}
.btn_yellow span {
    background: url("../images/btn_bgyellows2.png") no-repeat scroll right -502px transparent;
    cursor: pointer;
    display: block;
    height: 34px;
    line-height: 34px;
    padding-right: 20px;
}
.btn_grey {
    background: none repeat scroll 0 0 transparent;
    border: medium none;
    cursor: pointer;
    float: none;
    font-family: Tahoma;
    overflow: visible;
}

.btn_grey span {
    background: url("../images/btn_bgyellows2.png") no-repeat scroll left -76px transparent;
    cursor: pointer;
    display: block;
    height: 31px;
    line-height: 30px;
    padding: 0 0 0 7px;
}

.btn_grey span strong {
    background: url("../images/btn_bgyellows2.png") no-repeat scroll right -108px transparent;
    color: #333333;
    display: block;
    font-size: 14px;
    font-weight: normal;
    height: 31px;
    line-height: 30px;
    min-width: 110px;
    padding: 0 8px 0 0;
    text-shadow: 1px 1px 0 #FFFFFF;
    white-space: nowrap;
    width: auto !important;
}

.btn_yellow span strong {
    background: url("../images/btn_bgyellows2.png") no-repeat scroll left -468px transparent;
    color: #623500;
    display: block;
    font-size: 14px;
    height: 34px;
    line-height: 34px;
    min-width: 80px;
    padding: 0 0 0 20px;
    text-shadow: 1px 1px 0 #FFFFFF;
    white-space: nowrap;
    width: auto !important;
}
.firstDiscountTb{border-collapse: collapse;width: 460px;margin: 6px;color:#656565;}
