.current_nav{ height:22px; font-size:13px; color:#444;}
.current_nav span{ color:#a766ad}
.current_nav a{ color:#444;}

.mycashaccount{width:766px;height:auto;}
.mycashaccount p.mycashtit,.mycashaccount p.mycashtit1{border-bottom:2px solid #a368ac;line-height:30px;height:30px;margin-bottom:20px;}
.mycashaccount p.mycashtit strong,.mycashaccount p.mycashtit1 strong{float:left;font-size:14px;display:block;width:300px;color:#333;}
.coupontab{height:auto;overflow:hidden;border-bottom:1px solid #8ab4da;font-size:14px;font-weight:bold;margin-top:20px;}
.coupontab li{float:left;background:#ededed;background:-moz-linear-gradient(#fdfdfd,#e8e8e8);background:-o-linear-gradient(#fdfdfd,#e8e8e8)background:-webkit-gradient(linear,0 0,0 100%,from(#e8f5fe),to(#add5f8));padding:8px 20px;margin-right:10px;border:1px solid #d2d2d2;margin-top:10px;border-radius:6px 6px 0 0;border-bottom:0;cursor:pointer;}
.coupontab li.on{background:#c9e3fa;background:-webkit-gradient(linear,0 0,0 100%,from(#e8f5fe),to(#add5f8));background:-moz-linear-gradient(#e8f5fe,#add5f8);background:-o-linear-gradient(#e8f5fe,#add5f8);border:1px solid #8ab4da;border-bottom:0;}
.coupontable{display:none;}
.coupontable-one,.coupontable-two{border-collapse:collapse;width:100%;margin-bottom:20px;}
.coupontable-one th,.coupontable-two th{border:1px solid #cccccc;font-weight:normal;font-size:14px;color:#333;border-top:0;padding:7px 0;background:#f9f9f9;}
.coupontable-one td,.coupontable-two td{border:1px solid #cccccc;font-size:14px;color:#333;padding:7px 0;text-align:center;}
.coupontable-two th,.coupontable-two td{padding:7px 0;}
.coupontable.sh{display:block;}

.mycashaccount a.couponterm{text-decoration:none;color:red;font-weight: bold;}
.mycashaccount a.couponterm:hover{text-decoration:underline;}

.coupon-deadline{display:none;width:275px;border:1px solid #d0d1a9;position:absolute;background:#fcf8cb;z-index:100;padding:5px;right:-38px;top:22px;}
.coupon-deadline.sh{display:block;}
.coupon-deadline span{width:0; height:0; font-size:0; overflow:hidden; position:absolute;}
.coupon-deadline span.bot{border-width:6px;border-style:dashed dashed solid;border-color:transparent transparent #d0d1a9;left:235px;top:-12px;}
.coupon-deadline span.top{border-width:6px;border-style:dashed dashed solid;border-color:transparent transparent #ffc ;left:235px;top:-10px;}
.coupon-deadlinecont{display:inline-block;position:relative;}
.coupon-deadlineicon{background:url("../images/icon_shopcart.png");cursor: pointer;display: inline-block;height: 15px; width: 15px;}